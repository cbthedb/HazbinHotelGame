[
  {"id":"welcome","title":"Welcome to Hell","description":"Charlie greets you warmly.","type":"daily","choices":[{"id":"enthusiastic","text":"Match her energy!","outcomes":{"statChanges":{"empathy":2,"influence":1},"narrativeText":"Charlie beams with joy!"}},{"id":"polite","text":"Thank you for having me.","outcomes":{"statChanges":{"influence":2},"narrativeText":"A good start."}}],"onlyOnce":true},
